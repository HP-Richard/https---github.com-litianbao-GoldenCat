  <Code>Private calItems As Integer() = New Integer() { 5,9}
Public Function GetBgColor(ByVal itemId as Integer) As String
    Dim bgc as String = "无颜色"
  If(Array.IndexOf(Of Integer)(Me.calItems,itemId) &gt;=0 ) Then
        bgc="#b4c6e7"
    End If
    Return bgc
End Function

Private boldItems As Integer() = New Integer() {5,9 }
Public Function CheckBoldItemId(ByVal itemId As Integer) As Boolean
        Return (Array.IndexOf(Of Integer)(Me.boldItems, itemId) &gt;= 0)
End Function
